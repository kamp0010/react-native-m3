{
  "$schema": "https://nitro.margelo.com/nitro.schema.json",
  "cxxNamespace": [
    "m3"
  ],
  "ios": {
    "iosModuleName": "M3"
  },
  "android": {
    "androidNamespace": [
      "m3"
    ],
    "androidCxxLibName": "M3"
  },
  "autolinking": {
    "M3": {
      "swift": "HybridM3",
      "kotlin": "HybridM3"
    },
    "WavyProgressIndicatorView": {
      "swift": "HybridWavyProgressIndicatorView",
      "kotlin": "com.m3.HybridWavyProgressIndicatorView"
    },
    "WavySliderView": {
      "swift": "HybridWavySliderView",
      "kotlin": "com.m3.HybridWavySliderView"
    },
    "LyricsViewView": {
      "swift": "HybridLyricsView",
      "kotlin": "com.m3.HybridLyricsView"
    },
    "LoadingIndicatorView": {
      "swift": "HybridLoadingIndicatorView",
      "kotlin": "com.m3.HybridLoadingIndicatorView"
    }
  },
  "ignorePaths": [
    "**/node_modules"
  ]
}